{
  "name": "electron-browser",
  "version": "1.0.0",
  "description": "A minimal browser shell built on Electron.",
  "private": true,
  "main": "packages/shell/index.js",
  "workspaces": [
    "packages/shell",
    "packages/electron-chrome-extensions",
    "packages/electron-chrome-context-menu"
  ],
  "scripts": {
    "build": "yarn run build:context-menu && yarn run build:extensions && yarn run build:shell",
    "build:context-menu": "yarn --cwd ./packages/electron-chrome-context-menu build",
    "build:extensions": "yarn --cwd ./packages/electron-chrome-extensions build",
    "build:shell": "yarn --cwd ./packages/shell build",
    "start": "yarn build && yarn --cwd ./packages/shell start",
    "start:debug": "cross-env SHELL_DEBUG=true DEBUG='electron-chrome-extensions*' yarn start",
    "test": "yarn test:extensions && yarn test:shell",
    "test:extensions": "yarn --cwd ./packages/electron-chrome-extensions test",
    "test:shell": "yarn --cwd ./packages/shell test",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build:dmg": "electron-installer-dmg ./release-builds/electron-browser-darwin-x64/electron-browser.app Databank.me | MySearch --overwrite --icon=icons.icns --debug --out=./release-builds/",
    "build:win": "electron-packager . --platform=win32 --arch=x64 --icon=./path/to/icon.ico --electron-version=23.1.0 --prune=true --overwrite --out=./build",
    "sign": "electron-osx-sign --entitlements=my-entitlements.plist release-builds/electron-browser-darwin-x64/electron-browser.app",
    "build:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --icon=icons.icns --electron-version=23.1.0 --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=Electron-App --version-string.ProductName=\"Databank\"",
    "mac:sign": "electron-osx-sign release-builds/electron-browser-darwin-x64/electron-browser.app --identity='Developer ID Application: SuperWallet, Inc. (6T56ZC7QMH)'",
    "mac:pkg": "productbuild --component 'release-builds/electron-browser-darwin-x64/electron-browser.app' /Applications --sign '3rd Party Mac Developer Installer: SuperWallet, Inc. (6T56ZC7QMH)' 'electron-browser.pkg'"
  },
  "license": "GPL-3.0",
  "author": "Raza <raza@>",
  "dependencies": {
    "@electron/osx-sign": "^1.0.4",
    "axios": "^1.3.2",
    "electron-chrome-context-menu": "file:packages/electron-chrome-context-menu",
    "electron-chrome-extensions": "^3.10.0",
    "electron-debug": "^3.2.0",
    "electron-store": "^8.1.0",
    "fetch": "^1.1.0",
    "jquery": "^3.6.3",
    "node-fetch": "^3.3.0"
  },
  "build": {
    "appInfo": {
      "NSPhotoLibraryUsageDescription": "Your app needs access to the photo library"
    },
    "mac": {
      "target": "dmg",
      "icon": "icons.icns"
    }
  },
  "repository": "",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "jsonEnable": false,
    "semi": false,
    "endOfLine": "lf"
  },
  "devDependencies": {
    "electron-builder": "^23.6.0"
  }
}
